{
  "version": "1.0.0-*",
  "dependencies": {
    "Microsoft.Data.Sqlite": "1.0.0-*",
    "YesSql.Core": "2.0.0-*",
    "YesSql.Storage.LightningDB": "1.0.0-*",
    "xunit": "2.2.0-*"
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "debugType": "portable",
    "preserveCompilationContext": true
  },
  "commands": {
    "YesSql.Bench": "YesSql.Bench"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        },
        "System.Reflection": {
          "type": "build"
        },
        "System.Threading.Tasks": {
          "type": "build"
        }
      }
    },
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.0",
          "type": "platform"
        }
      }
    }
  }
}